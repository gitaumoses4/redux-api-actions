  "bugs": "https://github.com/gitaumoses4/redux-api-actions/issues",
  "keywords": [
    "redux",
    "reducer",
    "api",
    "hooks",
    "redux api actions",
    "reducer builder",
    "redux-saga",
    "react"
  ],
